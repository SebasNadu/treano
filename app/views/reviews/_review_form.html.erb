<div class="bg-trans p-3 mx-0 rounded">
  <%= simple_form_for([reviewable, review], class: "",
  data: {
  insert_in_list_target: "form",
     action: "submit->insert-in-list#send"
     }) do |f| %>
      <%= f.input :rating %>
      <%= f.input :content %>
      <%= f.submit "Add", class: "btn btn-outline-primary w-100" %>
   <% end %>
</div>
