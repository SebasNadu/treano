<div class="container">
  <p><%= @list.list_name %></p>
  <p><%= @list.description %></p>
  <p><%= @list.votes %></p>
  <% if @list.user.id == current_user.id %>
  <p><%= link_to "Edit list", edit_list_path(@list) %></p>
  <%= link_to "Delete",
  list_path(@list),
  data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}
  %>
  <% end %>
</div>
